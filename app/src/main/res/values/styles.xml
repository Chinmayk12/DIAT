<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="txtSolidRounded">
        <item name="android:textSize">@dimen/_18pxh</item>
        <item name="android:textColor">@color/white_A700</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textFontWeight" tools:targetApi="p">700</item>
        <item name="android:background">@drawable/rectangle_bg_gray_900_radius_23</item>
    </style>

    <style name="CustomOutlinedTextInput" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">@color/app_background2</item>
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
        <item name="hintTextColor">@color/app_background2</item>
        <item name="android:textColorHint">#878787</item>
        <item name="boxBackgroundColor">#FFFFFF</item>
    </style>


    <style name="groupStyleblue_100">
        <item name="android:background">@drawable/rectangle_bg_blue_100</item>
    </style>
</resources>
